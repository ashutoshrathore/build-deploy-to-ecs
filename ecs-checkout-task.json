{
  "family": "ecs-checkout-task",
  "requires_compatibilities" = ["FARGATE"]
  "cpu"                      = 512
  "memory"                   = 1024
  "containerDefinitions": [
    {
      "name": "checkout-test-container",
      "image": "557707468855.dkr.ecr.us-east-1.amazonaws.com/checkout-test-repo",
      "entryPoint": ["/"],
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}
